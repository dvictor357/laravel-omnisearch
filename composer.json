{
	"name": "dvictor357/laravel-omnisearch",
	"version": "1.0.0",
	"description": "A global command palette (Cmd+K) for Laravel applications. Search models, navigate routes, and execute commands from anywhere.",
	"keywords": [
		"laravel",
		"search",
		"command-palette",
		"spotlight",
		"livewire",
		"omnisearch"
	],
	"license": "MIT",
	"authors": [
		{
			"name": "dvictor357",
			"email": "dvictor3579@gmail.com"
		}
	],
	"require": {
		"php": "^8.2",
		"illuminate/support": "^11.0|^12.0",
		"livewire/livewire": "^3.0"
	},
	"require-dev": {
		"orchestra/testbench": "^9.0",
		"pestphp/pest": "^2.0"
	},
	"autoload": {
		"psr-4": {
			"OmniSearch\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"OmniSearch\\Tests\\": "tests/"
		}
	},
	"extra": {
		"laravel": {
			"providers": ["OmniSearch\\OmniSearchServiceProvider"],
			"aliases": {
				"OmniSearch": "OmniSearch\\Facades\\OmniSearch"
			}
		}
	},
	"config": {
		"sort-packages": true,
		"allow-plugins": {
			"pestphp/pest-plugin": true
		}
	},
	"minimum-stability": "stable",
	"prefer-stable": true
}
